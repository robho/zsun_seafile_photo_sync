SD_CARD_DEVICE=/dev/sda1
SD_CARD_MOUNT_POINT=/mnt
SD_CARD_IMAGE_DIRECTORY="$SD_CARD_MOUNT_POINT"
SD_CARD_TMP_DIRECTORY="$SD_CARD_MOUNT_POINT/.tmp"
SD_CARD_SWAP_FILE="$SD_CARD_MOUNT_POINT/.swapfile"

SYSFS_SD_CARD_SWITCH=/sys/class/gpio/sdreader_switch/value

